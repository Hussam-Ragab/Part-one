
/*
 * Write a program that reads a positive integer and checks if it is a perfect square.
 *
 * */

#include <stdio.h>

int main(void)
{
	int num1,num2,Result;
	printf("enter two numbers\n");
	fflush(stdout);
	scanf("%d%d",&num1,&num2);
	Result = ((num1 + num2) * 3) - 10;
	printf("Result = %d\n",Result);

	return 0;
}
