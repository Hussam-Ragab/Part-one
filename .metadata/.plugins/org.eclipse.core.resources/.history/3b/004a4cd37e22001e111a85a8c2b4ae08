/*
 *  Write a program to reverse a number.
 *
 * */

#include <stdio.h>
#define FALSE	0
#define TRUE	1

int main(void)
{
	int num, power,iterator,Result = 1;
	printf("enter number and power\n");
	fflush(stdout);
	scanf("%d%d",&num,&power);

	for(iterator = power; iterator > 0; --iterator)
	{
		Result *= num;
	}

	printf("Power of %d = %d\n",num,Result);

	return 0;
}
