/*
 * Write a program that print your name and your grade in a new line.
 *
 * */

#include <stdio.h>

int main(void)
{
	char name[20];
	float grade;
	printf("enter your name\n");
	fflush(stdout);
	scanf("%s",&name);
	printf("enter your grade\n");
	scanf("%f",&grade);
	printf("My Name %s\nMy Grade %.2f",name,grade);



	return 0;
}

